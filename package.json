{
  "name": "patadoc-marketing-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze",
    "test:accessibility": "node scripts/test-accessibility.js",
    "test:deployment": "node scripts/test-deployment.js",
    "deploy:validate": "node scripts/deploy.js",
    "deploy:build": "npm run deploy:validate && npm run build",
    "deploy:test": "set SKIP_BUILD_TEST=true && npm run test:deployment"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
